  "dependencies": {
    "@11ty/eleventy": "^0.11.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.1",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  }
}
